SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
ALTER PROCEDURE [dbo].[SP_UPD_CLIENTES]
	@PV_CEDULA VARCHAR (20),
	@PV_NOMBRES VARCHAR (100)
	
AS
BEGIN
	
	UPDATE CLIENTES
	SET NOMBRE=@PV_NOMBRES
	WHERE CEDULA=@PV_CEDULA

END