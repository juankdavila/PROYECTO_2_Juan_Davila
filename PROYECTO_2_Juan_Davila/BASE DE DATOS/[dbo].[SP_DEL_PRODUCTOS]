ALTER PROCEDURE [dbo].[SP_DEL_PRODUCTOS]
	@PV_CODIGO VARCHAR (20)
	

AS
	
BEGIN
	UPDATE PRODUCTOS
	SET ESTADO = 'I'
	WHERE
		CODIGO=@PV_CODIGO

END