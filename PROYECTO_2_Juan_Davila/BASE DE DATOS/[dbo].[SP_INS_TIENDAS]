ALTER PROCEDURE [dbo].[SP_INS_TIENDAS]
	@PV_ID_TIENDA INTEGER,
	@PV_DESCRIPCION VARCHAR (100)
	
AS
BEGIN
	
	INSERT INTO TIENDAS(ID_TIENDA,DESCRIPCION,ESTADO)
	VALUES(@PV_ID_TIENDA,@PV_DESCRIPCION,'A');

END