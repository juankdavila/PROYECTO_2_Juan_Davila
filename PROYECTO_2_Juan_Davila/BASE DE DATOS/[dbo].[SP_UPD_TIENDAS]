ALTER PROCEDURE [dbo].[SP_UPD_TIENDAS]
	@PV_ID_TIENDA INTEGER,
	@PV_DESCRIPCION VARCHAR (100),
	@PV_ESTADO VARCHAR (1)
	
AS
BEGIN
	
	UPDATE TIENDAS
	SET DESCRIPCION=@PV_DESCRIPCION,
    ESTADO=@PV_ESTADO
	WHERE ID_TIENDA=@PV_ID_TIENDA

END