ALTER PROCEDURE [dbo].[SP_GET_PROMOCION]
	@PI_ID_PROMOCION INTEGER
AS
BEGIN
	
	SELECT * FROM PROMOCIONES
	WHERE ID_PROMOCION=@PI_ID_PROMOCION

END