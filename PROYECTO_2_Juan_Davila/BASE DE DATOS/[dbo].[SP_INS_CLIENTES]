ALTER PROCEDURE [dbo].[SP_INS_CLIENTES]
	@PV_CEDULA VARCHAR (20),
	@PV_NOMBRES VARCHAR (100)
	
AS
BEGIN
	
	INSERT INTO CLIENTES(CEDULA,NOMBRE,ESTADO)
	VALUES(@PV_CEDULA,@PV_NOMBRES,'A');

END