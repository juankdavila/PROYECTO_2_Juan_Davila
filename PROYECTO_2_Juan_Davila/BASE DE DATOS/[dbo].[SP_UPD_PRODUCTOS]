ALTER PROCEDURE [dbo].[SP_UPD_PRODUCTOS]
	@PV_CODIGO VARCHAR (20),
	@PV_DESCRIPCION VARCHAR (100),
	@PF_PRECIO DECIMAL(10,2)
	

AS
	
BEGIN
	UPDATE PRODUCTOS
	SET DESCRIPCION=@PV_DESCRIPCION,
		PRECIO=@PF_PRECIO
	WHERE
		CODIGO=@PV_CODIGO

END