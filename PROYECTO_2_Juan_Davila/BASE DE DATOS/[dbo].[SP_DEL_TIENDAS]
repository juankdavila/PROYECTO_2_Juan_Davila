ALTER PROCEDURE [dbo].[SP_DEL_TIENDAS]
	@PV_ID_TIENDA INTEGER 
	
AS
BEGIN
	
	UPDATE TIENDAS
	SET ESTADO='I'
	WHERE ID_TIENDA=@PV_ID_TIENDA

END